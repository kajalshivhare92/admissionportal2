<%- include('../header.ejs') %>
    <br><br><br><br><br>
    <br><br>
    <div class="container">
        <div class="alert alert-success">
            <h4>Contact Display</h4>
        </div>
        <div class="table-responsive">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Mobile Number</th>
                        <th>Message</th>
                    </tr>
                </thead>
                <tbody>
                    <% d.forEach((item)=>{ %>
                        <tr>
                            <td>
                                <%= item._id %>
                            </td>
                            <td>
                                <%= item.name %>
                            </td>
                            <td>
                                <%= item.email %>
                            </td>
                            <td>
                                <%= item.phone %>
                            </td>
                            <td>
                                <%= item.message %>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>
    </div>
    <%- include('../footer.ejs') %>